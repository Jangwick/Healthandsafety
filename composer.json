{
    "name": "lgu/health-safety-inspection",
    "description": "Health & Safety Inspection System for LGU",
    "type": "project",
    "require": {
        "php": ">=8.2",
        "firebase/php-jwt": "^6.10",
        "phpmailer/phpmailer": "^7.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\": "database/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        },
        "audit": {
            "block-insecure": false
        }
    }
}
